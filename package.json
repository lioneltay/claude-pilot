{
  "name": "claude-proxy",
  "version": "1.0.0",
  "private": true,
  "description": "Proxy Claude Code requests through GitHub Copilot",
  "scripts": {
    "dev": "turbo dev",
    "dev:proxy": "turbo dev --filter=@claude-proxy/proxy",
    "dev:dashboard": "turbo dev --filter=@claude-proxy/dashboard",
    "start": "turbo start",
    "auth": "pnpm --filter @claude-proxy/proxy auth",
    "typecheck": "turbo typecheck",
    "logs:clear": "rm -f logs/requests.jsonl && echo 'Logs cleared'"
  },
  "devDependencies": {
    "turbo": "^2.5.4"
  },
  "packageManager": "pnpm@10.27.0"
}
